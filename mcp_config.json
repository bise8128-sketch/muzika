{
    "mcpServers": {
        "github.com/upstash/context7-mcp": {
            "command": "npx",
            "args": [
                "-y",
                "@upstash/context7-mcp",
                "--api-key",
                "${UPSTASH_API_KEY}"
            ]
        },
        "github": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-github"
            ],
            "env": {
                "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_PERSONAL_ACCESS_TOKEN}"
            }
        },
        "filesystem": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-filesystem",
                "/home/k/Downloads/muzika"
            ]
        },
        "brave-search": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-brave-search"
            ],
            "env": {
                "BRAVE_SEARCH_API_KEY": "BSAotF0rYCjZm7xgrhfQHoEwcfG1RnI"
            }
        },
        "docker": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-docker"
            ]
        },
        "puppeteer": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-puppeteer"
            ]
        },
        "node": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-node"
            ]
        },
        "ffmpeg": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-ffmpeg"
            ]
        }
    }
}
